<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="688.0" prefWidth="926.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hitec.nhplus.presenter.UserLoginPresenter">
    <GridPane layoutX="363.0" layoutY="291.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <Label text="User" />
        <Label text="Password" GridPane.rowIndex="1" />
        <TextField fx:id="username" GridPane.columnIndex="1" />
        <TextField fx:id="userPassword" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    </GridPane>
    <Button fx:id="buttonLogin" layoutX="437.0" layoutY="382.0" mnemonicParsing="false" onAction="#handleLogin" text="Login" />
    <Label alignment="CENTER" layoutX="405.0" layoutY="211.0" prefHeight="80.0" prefWidth="109.0" text="Login">
        <font>
            <Font name="System Bold" size="31.0" />
        </font>
    </Label>
</AnchorPane>
